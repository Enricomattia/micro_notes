@article{anderssonParityComparabilityConcern2016,
  title = {Parity and {{Comparability}}: A {{Concern Regarding Chang}}'s {{Chaining Argument}}},
  author = {Andersson, Henrik},
  year = {2016},
  journal = {Ethical Theory and Moral Practice},
  volume = {19},
  number = {1},
  pages = {245--253},
  issn = {1572-8447},
  abstract = {According to Ruth Chang the three standard positive value relations: ``better than'', ``worse than'' and ``equally good'' do not fully exhaust the conceptual space for positive value relations. According to her, there is room for a fourth positive value relation, which she calls ``parity''. Her argument for parity comes in three parts. First, she argues that there are items that are not related by the standard three value relations. Second, that these items are not incomparable, and third, that the phenomena she has focused on are not due to the vagueness of the comparative predicates (i.e., that it is indeterminate which of the standard value relations that holds). This paper focuses on the second part of the argument and an objection is presented. By assuming the Small Unidimensional Difference Principle, which is a key premise for the second part of the argument, Chang's argument could be accused of begging the question. More so, by assuming this principle, the space for incomparability gets severely limited. If these worries are justified, then Chang's argument for parity as a fourth form of comparability is unsuccessful.},
  langid = {english},
  keywords = {Begging the question,Chang,Comparability,Comparisons,Parity,Trichotomy,Vagueness,Value relations}
}

